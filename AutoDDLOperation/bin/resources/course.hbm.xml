<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

<class name  = "beans.Course" table = "course" schema="test">
<id   name   = "cid" column = "CourseID" type = "integer"/>

<property name = "cname" column = "CourseName"></property>
<property name = "cfee"  column = "CourseFee"></property>

</class>
</hibernate-mapping>
