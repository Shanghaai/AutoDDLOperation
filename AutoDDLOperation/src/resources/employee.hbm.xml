<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
  
<hibernate-mapping>

<class name  = "beans.Employee" table = "employee" schema="test">
   <id name = "id" column = "EmployeeID"/>

<property name="name" column = "EmployeeName"></property>
<property name="email" column = "EmployeeEmail"></property>


</class>
</hibernate-mapping>
